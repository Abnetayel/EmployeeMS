@model List<WebApplication3.Models.Attendance>

<h1>Attendance Records</h1>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Is Present</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var record in Model)
        {
            <tr>
                <td>@record.Date.ToShortDateString()</td>
                <td>@(record.IsPresent ? "Yes" : "No")</td>
            </tr>
        }
    </tbody>
</table>
